<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.apps.ivonne.primertarea">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".RelativeLayout">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".FrameLayout" />
        <activity android:name=".GridLayout" />
        <activity android:name=".TableLayout" />
        <activity android:name=".linearLayout" />
        <activity android:name=".Fragment" />
        <activity android:name=".AbsoluteLayout" />
        <activity android:name=".ConstraintLayout"></activity>
    </application>

</manifest>